<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/menu_layout" android:layout_width="wrap_content"
	android:layout_height="100dp" android:orientation="vertical"
	android:gravity="center_horizontal">
	<ImageView android:layout_width="64dp" android:layout_marginTop="15dp"
		android:layout_height="64dp" android:id="@+id/menu_image"></ImageView>
	<TextView android:layout_width="wrap_content" android:gravity="center"
		android:layout_height="wrap_content" android:id="@+id/menu_text"
		android:textColor="@color/black" android:textSize="18dp"></TextView>
</LinearLayout>
